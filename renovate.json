{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:base"],
  "platformAutomerge": true,
  "pre-commit": {
    "enabled": true
  },
  "enabledManagers": ["pre-commit", "terraform"],
  "terraform": {
    "fileMatch": ["^/terraform/.*\\.tf$"]
  },
  "prHourlyLimit": 20
}
