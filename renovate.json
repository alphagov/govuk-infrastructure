{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "platformAutomerge": true,
  "pre-commit": {
    "enabled": true
  },
  "enabledManagers": [
    "pre-commit",
    "terraform"
  ],
  "terraform": {
    "fileMatch": [
      "^/terraform/.*\\.tf$"
    ]
  },
  "prHourlyLimit": 20
}
