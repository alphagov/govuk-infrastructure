{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    "helpers:pinGitHubActionDigests"
  ],
  "platformAutomerge": true,
  "pre-commit": {
    "enabled": true
  },
  "enabledManagers": [
    "github-actions",
    "pre-commit",
    "terraform"
  ],
  "terraform": {
    "managerFilePatterns": [
      "/^/terraform/.*\\.tf$/"
    ]
  },
  "prHourlyLimit": 20,
  "labels": [
    "dependencies",
    "renovate"
  ]
}
